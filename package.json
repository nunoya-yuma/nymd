{
  "name": "nymd",
  "displayName": "NYMD - Markdown Editor",
  "description": "WYSIWYG-style Markdown editor with live preview and toolbar",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "nymd.openEditor",
        "title": "Open NYMD Editor",
        "category": "NYMD"
      }
    ],
    "customEditors": [
      {
        "viewType": "nymd.markdownEditor",
        "displayName": "NYMD Markdown Editor",
        "selector": [
          {
            "filenamePattern": "*.md"
          }
        ]
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "nymd.openEditor",
          "when": "resourceExtname == .md",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}